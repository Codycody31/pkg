{
  "name": "go",
  "description": "The Go programming language",
  "homepage": "https://go.dev",
  "version": "1.24.6",
  "sha256": "4e29202c49573b953be7cc3500e1f8d9e66ddd12faa8cf0939a4951411e09a2a",
  "url": "https://go.dev/dl/go{{ version }}.darwin-arm64.tar.gz",
  "caveats": "This package does not have shell completions!",
  "scripts": {
    "install": [
      "tar -xf go{{ version }}.darwin-arm64.tar.gz go",
      "mv go {{ pkg.opt_dir }}/go",
      "ln -sf {{ pkg.opt_dir }}/go/bin/go {{ pkg.bin_dir }}/go",
      "ln -sf {{ pkg.opt_dir }}/go/bin/gofmt {{ pkg.bin_dir }}/gofmt"
    ],
    "latest": [
      "curl -fsSL \"https://go.dev/VERSION?m=text\" | grep 'go*' | cut -c 3-"
    ]
  }
}
